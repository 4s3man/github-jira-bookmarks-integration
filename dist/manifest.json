{
    "name": "CR alert",
    "version": "0.0.1",
    "manifest_version": 2,
    "description": "Easy check multiple bookmarked sites, for changes requested after cr",
    "homepage_url": "https://soundcloud.com/kuba-ku-aga",
    "icons": {
      "16": "icons/icon16.png",
      "48": "icons/icon48.png",
      "128": "icons/icon128.png"
    },
    "commands": {
      "save" : {
        "suggested_key": {
          "default": "Alt+Shift+S"
        },
        "description": "Save a link"
      },
      "random": {
        "suggested_key": {
          "default": "Alt+Shift+L"
        },
        "description": "Load a random link"
      }
    },
    "browser_action": {
      "default_title": "TSRWPCX",
      "default_popup": "popup.html"
    },
    "default_locale": "en",
    "background": {
      "scripts": [
        "js/background.js"
      ],
      "persistent": true
    },
    "permissions": [
      "bookmarks",
      "https://*/*"
    ],
    "content_security_policy": "unsafe-inline"
  }

